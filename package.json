{
  "name": "openrecruitment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "CHOKIDAR_USEPOLLING=true  next dev",
    "build": "npm install -g dotenv-cli && prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && npx prisma format",
    "cypress": "cypress run",
    "prismaMigrateLocal": "dotenv -e .env.local -- npx prisma migrate dev",
    "prismaGenerate": "dotenv -e .env.local -- npx prisma generate",
    "prismaMigrateLocalNoSeed": "dotenv -e .env.local -- npx prisma migrate dev --skip-seed",
    "prismaDBSeed": "dotenv -e .env.local -- npx prisma db seed",
    "prismaDBReset": "dotenv -e .env.local -- npx prisma db push --force-reset && dotenv -e .env.local -- npx prisma db seed",
    "vercelBuild": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@headlessui/react": "1.7.15",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.14.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "clsx": "1.2.1",
    "eslint-config-next": "13.4.2",
    "framer-motion": "10.12.16",
    "graphql": "^16.6.0",
    "graphql-yoga": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "13.4.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "react-phone-input-2": "^2.15.1",
    "react-tooltip": "5.18.0",
    "run": "1.4.0",
    "sweetalert2": "11.4.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.0",
    "@as-integrations/next": "^2.0.1",
    "@graphql-authz/apollo-server-plugin": "^3.1.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-authz": "^3.5.8",
    "@pothos/plugin-prisma": "^3.52.0",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "cypress": "12.12.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-prisma": "4.13.0",
    "prisma": "^4.14.0",
    "prisma-generator-pothos-codegen": "^0.5.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
