{
  "name": "openrecruitment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && npx prisma format",
    "cypress": "cypress run",
    "prismaMigrateLocal": "dotenv -e .env.local -- npx prisma migrate dev",
    "prismaGenerate": "dotenv -e .env.local -- npx prisma generate",
    "prismaMigrateLocalNoSeed": "dotenv -e .env.local -- npx prisma migrate dev --skip-seed",
    "prismaDBSeed": "dotenv -e .env.local -- npx prisma db seed",
    "prismaDBReset": "dotenv -e .env.local -- npx prisma db push --force-reset && dotenv -e .env.local -- npx prisma db seed",
    "prismaStudio": "dotenv -e .env.local -- npx prisma studio",
    "vercelBuildReset": "npx prisma migrate reset --force && npx prisma generate && next build",
    "vercelBuild": "npx prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datamodel prisma/schema.prisma --script > down.sql && npx prisma generate && next build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@faker-js/faker": "8.0.2",
    "@headlessui/react": "1.7.15",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.9.7",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "eslint-config-next": "13.4.2",
    "framer-motion": "10.12.16",
    "graphql": "^16.6.0",
    "html-react-parser": "^5.0.1",
    "lodash": "^4.17.21",
    "next": "13.4.2",
    "next-query-params": "^4.2.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-big-calendar": "1.8.4",
    "react-calendar": "4.6.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-google-charts": "4.0.1",
    "react-icons": "4.10.1",
    "react-pdf": "^7.5.1",
    "react-phone-input-2": "2.15.1",
    "react-simple-wysiwyg": "^2.2.5",
    "react-tooltip": "5.18.0",
    "react-use": "^17.4.0",
    "run": "1.4.0",
    "sanitize-html": "^2.11.0",
    "sweetalert2": "11.4.8",
    "swr": "2.2.4",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.0",
    "@as-integrations/next": "^2.0.1",
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/s3-request-presigner": "^3.433.0",
    "@graphql-authz/apollo-server-plugin": "^3.1.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-authz": "^3.5.8",
    "@pothos/plugin-prisma": "^3.52.0",
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/formidable": "^3.4.3",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.4.1",
    "@types/react-big-calendar": "^1.6.4",
    "@types/sanitize-html": "^2.9.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "cypress": "12.12.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "formidable": "^3.5.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-prisma": "4.13.0",
    "prisma": "^5.2.0",
    "prisma-generator-pothos-codegen": "^0.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "overrides": {
    "prisma-generator-pothos-codegen": {
      "@prisma/client": "$@prisma/client",
      "prisma": "$prisma"
    }
  }
}
